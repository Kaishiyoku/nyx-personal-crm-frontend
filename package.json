{
  "name": "react-webpack-babel-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement) and Babel.",
  "keywords": [
    "react",
    "webpack",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-babel-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-babel-starter/issues"
  },
  "homepage": "https://github.com/vikpe/react-webpack-babel-starter",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod",
    "clean-dist": "rimraf dist/*",
    "prettier-write": "prettier --write \"{src,tests}/**/*.{js,jsx,css,scss}\"",
    "prettier-check": "prettier --list-different \"{src,tests}/**/*.{js,jsx,css,scss}\"",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watchAll --coverage --config=configs/jest.json",
    "lint": "eslint ./src --ext .js,.jsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@reach/router": "^1.3.4",
    "@react-md/material-icons": "^2.0.0",
    "antd": "^4.4.1",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^3.6.0",
    "deepmerge": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.3.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-no-switch-statements": "^1.0.0",
    "eslint-plugin-no-unsafe-chars": "^1.0.2",
    "file-loader": "^6.0.0",
    "formsy-react": "^2.0.3",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "is-plain-object": "^3.0.1",
    "jest": "^26.1.0",
    "less-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "node-polyglot": "^2.4.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-md": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "sass-loader": "^8.0.2",
    "somereactcomponents": "^0.1.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
